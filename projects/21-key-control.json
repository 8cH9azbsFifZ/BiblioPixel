{
    "aliases": {
        "bpa": "BiblioPixelAnimations.strip"
    },

    "controls": {
        "typename": "keyboard",
        "verbose": true,
        "extractor": {
            "accept": {"type": "press"}
        },
        "routing": {
            "a": "animation.index = Alternates",
            "s": "animation.index = FireFlies",
            "d": "animation.index = Wave",
            "f": "animation.index = WhiteTwinkle"
        }
    },

    "dimensions": [50],

    "run": {
        "fps": 30
    },

    "animation": {
        "typename": "sequence",
        "animations": [
            "@bpa.Alternates",
            "@bpa.BinaryEpochClock.BEClock",
            "@bpa.ColorChase",
            "@bpa.ColorFade",
            "@bpa.ColorPattern",
            "@bpa.ColorWipe",
            "@bpa.FireFlies",
            "@bpa.HalvesRainbow",
            "@bpa.LarsonScanners.LarsonRainbow",
            "@bpa.LarsonScanners.LarsonScanner",
            "@bpa.LinearRainbow",
            "@bpa.PartyMode",
            "@bpa.PixelPingPong",
            "@bpa.RGBClock",
            "@bpa.Rainbows.Rainbow",
            "@bpa.Rainbows.RainbowCycle",
            "@bpa.Searchlights",
            "@bpa.Wave",
            "@bpa.Wave.WaveMove",
            "@bpa.WhiteTwinkle",
            "@bpa.hexclock"
        ]
    }
}
